<h1>Home</h1>
<% if session[:user_id] %>
  <h1><%= @user.name %></h1>
  <h2>Email: <%= @user.email %></h2>
  <h2>Facebook UID: <%= @user.uid %></h2>
  <img src="<%= @user.image %>">
<% else %>
  <%= link_to('Log in with Facebook!', '/auth/facebook') %><br>
  <%= link_to 'Log in with Sunday School!', login_path(@teacher)%> 
  <p>Don't have an account? <%= link_to "Sign Up", new_teacher_path %></p>
<% end %>