<h1>Log in Here!</h1>
<%= form_tag(sessions_path) do %>
    <%= label_tag :email %><br>
    <%= email_field_tag :email %><br><br>
    <%= label_tag :password %><br>
    <%= password_field_tag :password %><br><br>
    <%= submit_tag "Log In" %><br><br>
<% end %> 
<p>Don't have an account? <%= link_to "Sign Up", new_teacher_path %></p>
  <p><%= link_to "Return Home", root_path %></p>